<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>

<!--
  http://urlrewritefilter.googlecode.com/svn/trunk/src/doc/manual/4.0/index.html
-->

<!-- make sure IE gets the no compatibility mode message -->
<rule>
  <!-- <condition name="person-agent">what is the IE person agent?</from> -->
  <from>.*</from>
  <set type="response-header" name="X-UA-Compatible">IE=Edge</set>
</rule>

</urlrewrite>
